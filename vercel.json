{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {"source": "/api/pay/ltzf/notify", "destination": "/api/pay/ltzf/notify"},
    {"source": "/api/bet-proxy", "destination": "https://api.amjsvip.cc/api/bet-proxy"},
    {"source": "/api/:path*", "destination": "https://api.b772026.com/api/:path*"},
    {"source": "/(.*)", "destination": "/index.html"}
  ],
  "headers": [
    {"source": "/assets/(.*)", "headers": [{"key": "Cache-Control", "value": "public, max-age=31536000, immutable"}]},
    {"source": "/(.*)", "headers": [{"key": "X-Content-Type-Options", "value": "nosniff"}, {"key": "X-Frame-Options", "value": "SAMEORIGIN"}]}
  ]
}
